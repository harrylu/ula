<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="admin/reservation" extends="global" namespace="/reservation">
	
		<action name="hotelReserv" class="ula.action.admin.reservation.HotelReservation">
		<result name ="success">reservation_result.ftl</result>
		<result name ="admin">/admin/reservation/hotel_admin.ftl</result>
		<result name ="detail">/admin/reservation/hotel_detail.ftl</result>
		<result name="view">hotel_reservation.ftl</result>
		</action>
		
		<action name="travelReserv" class="ula.action.admin.reservation.TravelReservation">
		<result name ="success">reservation_result.ftl</result>
		<result name ="admin">/admin/reservation/travel_admin.ftl</result>
		<result name ="detail">/admin/reservation/travel_detail.ftl</result>
		<result name="view">travel_reservation.ftl</result>
		</action>
	
	</package>
	

</struts>
