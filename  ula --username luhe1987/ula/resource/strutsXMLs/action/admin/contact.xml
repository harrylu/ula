<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="admin/contact" extends="global" namespace="/admin/contact">
	
	
<!--联系我们->个人客户 	-->
		<action name="person" class="ula.action.admin.contact.IndividualClientAction">
		<result name="edit">person_edit.jsp</result>
		<result name="view">person_view.jsp</result>
		<result name="update">person_admin.jsp</result>
		<result name="admin">person_admin.jsp</result>
		<result name="preAdd">person_add.jsp</result>
		</action>

<!--联系我们->俄罗斯团体客户 	-->		
		<action name="rugroup" class="ula.action.admin.contact.GroupClientRussianAction">
		<result name="edit">rugroup_edit.jsp</result>
		<result name="view">rugroup_view.jsp</result>
		<result name="update">rugroup_admin.jsp</result>	
		<result name="admin">rugroup_admin.jsp</result>
		<result name="preAdd">rugroup_add.jsp</result>
		</action>
		
		
<!--联系我们->中国团体客户 	-->	
		<action name="cngroup" class="ula.action.admin.contact.GroupClientChineseAction">
		<result name="edit">cngroup_edit.jsp</result>
		<result name="view">cngroup_view.jsp</result>
		<result name="update">cngroup_admin.jsp</result>
		<result name="admin">cngroup_admin.jsp</result>	
		<result name="preAdd">cngroup_add.jsp</result>
		</action>
		
	</package>
	
</struts>
